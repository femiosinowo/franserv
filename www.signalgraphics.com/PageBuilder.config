<?xml version="1.0" encoding="utf-8" ?>
<PageBuilderConfiguration ResizeDraggingEnabled="false">
    <Sizes>
        <SizeType Name="Pixels" EnumVal="0" Label="Width in pixels:" Enabled="true" />
        <SizeType Name="Em" EnumVal="1" Label="Width in em:" Enabled="true" />
        <SizeType Name="Percent" EnumVal="2" Label="Percent width:" Enabled="true" />
        <SizeType Name="Bootstrap" EnumVal="3" Label="Bootstrap" Enabled="true" DropZoneOuterClass="container" DropZoneClass="row">
            <Group Name="positioning" Label="Positioning">
                <DropDown Name="width" Label="Width:">
                    <CssClass ClassName="span1" Label="1 Column" />
                    <CssClass ClassName="span2" Label="2 Columns" />
                    <CssClass ClassName="span3" Label="3 Columns" />
                    <CssClass ClassName="span4" Label="4 Columns" />
                    <CssClass ClassName="span5" Label="5 Columns" />
                    <CssClass ClassName="span6" Label="6 Columns" />
                    <CssClass ClassName="span7" Label="7 Columns" />
                    <CssClass ClassName="span8" Label="8 Columns" />
                    <CssClass ClassName="span9" Label="9 Columns" />
                    <CssClass ClassName="span10" Label="10 Columns" />
                    <CssClass ClassName="span11" Label="11 Columns" />
                    <CssClass ClassName="span12" Label="12 Columns" />
                    <CssClass ClassName="rowsplit" Label="Split Row" />
                </DropDown>
                <DropDown Name="offset" Label="Offset:">
                    <CssClass ClassName="" Label="No Offset" />
                    <CssClass ClassName="offset1" Label="1 Column" />
                    <CssClass ClassName="offset2" Label="2 Columns" />
                    <CssClass ClassName="offset3" Label="3 Columns" />
                    <CssClass ClassName="offset4" Label="4 Columns" />
                    <CssClass ClassName="offset5" Label="5 Columns" />
                    <CssClass ClassName="offset6" Label="6 Columns" />
                    <CssClass ClassName="offset7" Label="7 Columns" />
                    <CssClass ClassName="offset8" Label="8 Columns" />
                    <CssClass ClassName="offset9" Label="9 Columns" />
                    <CssClass ClassName="offset10" Label="10 Columns" />
                    <CssClass ClassName="offset11" Label="11 Columns" />
                </DropDown>
            </Group>
            <Group Name="visibility" Label="Visibility">
                <DropDown Name="phone" Label="Phone:">
                    <CssClass ClassName="" Label="Default" />
                    <CssClass ClassName="visible-phone" Label="Visible" />
                    <CssClass ClassName="hidden-phone" Label="Invisible" />
                </DropDown>
                <DropDown Name="tablet" Label="Tablet:">
                    <CssClass ClassName="" Label="Default" />
                    <CssClass ClassName="visible-tablet" Label="Visible" />
                    <CssClass ClassName="hidden-tablet" Label="Invisible" />
                </DropDown>
                <DropDown Name="desktop" Label="Desktop:">
                    <CssClass ClassName="" Label="Default" />
                    <CssClass ClassName="visible-desktop" Label="Visible" />
                    <CssClass ClassName="hidden-desktop" Label="Invisible" />
                </DropDown>
            </Group>
            <CssInclude Path="~/workarea/FrameworkUI/css/bootstrap/css/bootstrap.css" />
            <CssInclude Path="~/workarea/FrameworkUI/css/bootstrap/css/bootstrap-responsive.css" />
            <CssInclude Path="~/workarea/FrameworkUI/css/bootstrap/css/bootstrap-compatibility.css" />
        </SizeType>
        <SizeType Name="Foundation" EnumVal="3" Label="Foundation" Enabled="false" DropZoneOuterClass="" DropZoneClass="row" ColumnClass="columns">
            <Group Name="largedisplay" Label="Large Display">
                <DropDown Name="largewidth" Label="Width:">
                    <CssClass ClassName="large-1" Label="1 Column" />
                    <CssClass ClassName="large-2" Label="2 Columns" />
                    <CssClass ClassName="large-3" Label="3 Columns" />
                    <CssClass ClassName="large-4" Label="4 Columns" />
                    <CssClass ClassName="large-5" Label="5 Columns" />
                    <CssClass ClassName="large-6" Label="6 Columns" />
                    <CssClass ClassName="large-7" Label="7 Columns" />
                    <CssClass ClassName="large-8" Label="8 Columns" />
                    <CssClass ClassName="large-9" Label="9 Columns" />
                    <CssClass ClassName="large-10" Label="10 Columns" />
                    <CssClass ClassName="large-11" Label="11 Columns" />
                    <CssClass ClassName="large-12" Label="12 Columns" />
                </DropDown>
                <DropDown Name="largeoffset" Label="Offset:">
                    <CssClass ClassName="" Label="None" />
                    <CssClass ClassName="large-offset-1" Label="1 Column" />
                    <CssClass ClassName="large-offset-2" Label="2 Columns" />
                    <CssClass ClassName="large-offset-3" Label="3 Columns" />
                    <CssClass ClassName="large-offset-4" Label="4 Columns" />
                    <CssClass ClassName="large-offset-5" Label="5 Columns" />
                    <CssClass ClassName="large-offset-6" Label="6 Columns" />
                    <CssClass ClassName="large-offset-7" Label="7 Columns" />
                    <CssClass ClassName="large-offset-8" Label="8 Columns" />
